{
  "version": 2,
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php"
    },
    {
      "src": "api/*.php",
      "use": "vercel-php"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.php"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
